<h1>Sign up</h1>

<%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label :img_url %>
    <%= f.text_field :img_url %>

    <%= f.label :motto %>
    <%= f.text_field :motto %>

    <%= f.submit %>
<% end %>